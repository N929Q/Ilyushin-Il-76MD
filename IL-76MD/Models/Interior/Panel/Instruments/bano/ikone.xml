<PropertyList>
 <path>ikone.ac</path>
<!--External panel lighting-->
<animation>
  <type>material</type>  
  <object-name>base_4</object-name>  
    <emission>
      <factor-prop>controls/switches/gauge-light</factor-prop>
      <offset>0.1</offset>
      <red>   0.11 </red>
      <green> 0.11 </green>
      <blue>  0.11 </blue>
    </emission>
</animation>

<animation>
 <type>select</type>	  
  <condition>
    <property>sim/initialized</property>
  </condition>
</animation>

</PropertyList>

